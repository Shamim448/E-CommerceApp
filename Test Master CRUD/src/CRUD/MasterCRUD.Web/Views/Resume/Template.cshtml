
@section Styles{
    <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/cropperjs/1.6.0/cropper.css" />
    <link href="~/css/cvbuilder-one.css" rel="stylesheet" />
    <style>
        .mkp_container{               
        margin: auto;
        margin-top: 146px;
        margin-bttom: 146px;
        padding-left: 0;
        padding-right: 0;
        width:100%;
        }
  
    </style>
}
<div class="container">
    <div class="mkp_container" id="main-container">
        <header class="mkp__header">
            <div class="mkp__header__information">

                <address>
                    <h3 class="mkp__font__bold editable cvName">@Model.FName @Model.LName</h3>
                    <p>
                        <strong class="mkp__font__bold  editable">Email:</strong>
                        <a class="mkp__href email editable" href="mailto:jalal.test@gmail.com">@Model.Email</a>
                    </p>
                    <p>
                        <strong class="mkp__font__bold editable">Contact: </strong><a class="mkp__href editable mobile"
                                                                                      href="tel:+8801711111111">@Model.PhoneNo</a>
                    </p>
                    <p>
                        <strong class="mkp__font__bold editable socialName">Skype: </strong> <span class="editable sociallink">Shamim.448</span>
                    </p>
                    <p>
                        <strong class="mkp__font__bold editable socialName"> LinkedIn: </strong>

                        <a class="mkp__href editable sociallink"
                           href="#">https://www.linkedin.com/sashamim/</a>
                    </p>
                    <h5 class="editable address">
                       @Model.Address
                    </h5>
                </address>
            </div>
            <div class="mkp__header__image">
                <img id="image-upload" src="~/images/noimage.png" alt="Not found Image" /> 
            </div>
        </header>
        <main class="mkp__main">
             <section class="mkp__eduction">
                <h4 class="mkp__font__bold mkp__heading_h4 editable">Education</h4>
                <div class="mkp__education__container">
                    <p class="editable subject">
                        Master of Science in Computer Science (90%) Asian University,
                        Dhaka, Bangladesh
                    </p class="editable">
                    <p class="text-right passing-year editable">2005</p>
                </div>
                <div class="mkp__education__container">
                    <p class="editable subject">
                        B.Sc. in Computer Science & Engineering (87%) American
                        International University - Bangladesh
                    </p>
                    <p class="text-right editable passing-year">2000</p>
                </div>
                <div class="mkp__education__container">
                    <p class="editable subject">
                        Higher Secondary (H.S.C) in Science Group (2nd Division) Rangpur
                        Govt. College, Rangpur
                    </p>
                    <p class="text-right editable passing-year">1996</p>
                </div>

                <div class="mkp__education__container">
                    <p class="editable subject">
                        Secondary School (S.S.C) in Science Group (1st Division) Rangpur
                        Modern Boys High School, Chittagong
                    </p>
                    <p class="text-right editable passing-year">1990</p>
                </div>
            </section>
        </main>
    </div>
</div>

