@model IList<Category>

<h1>Category List</h1>
<table class="table table-bordered table-striped">
    <thead>
        <tr>
            <th>Name</th>
            <th>DisplayOrder</th>
            <th>ID</th>
        </tr>
    </thead>
    <tbody>
        
        @foreach (var obj in Model.OrderBy(O => O.DisplayOrder))
        {
            <tr>
                <td>@obj.Name</td>
                <td>@obj.DisplayOrder</td>
                <td>@obj.Id</td>
            </tr>
        }
            
        
        
    </tbody>
</table>
